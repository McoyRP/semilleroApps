
datos <- read.delim("C:/Users/fhbapto/Desktop/semilleroApps/app de prueba ph dif prop/datos.txt")

dim(datos)

indices <- sample(1:nrow(datos), size=500, replace=FALSE)

dt1 <- datos[ indices, ]
dt2 <- datos[-indices, ]

write(x=t(dt1), file='datos1.txt', ncolumns=6, sep='\t')
write(x=t(dt2), file='datos2.txt', ncolumns=6, sep='\t')
